{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "jasmine": false,
    "node": true,
    "browser": false,
    "builtin": true,
    "jquery": false
  },
  "globals": {
  },
  "plugins": [
      "eslint-plugin-random"
  ],
  "ecmaFeatures": {
    "jsx": false,
  },
  "rules": {
    "comma-dangle": 0,
    "block-scoped-var": 2,
    "camelcase": [2, {"properties": "always"}],
    "curly": [ 2, "all" ],
    "dot-notation": [
      2,
      {
        "allowKeywords": true
      }
    ],
    "eqeqeq": [
      2,
      "allow-null"
    ],
    "global-strict": [
      2,
      "never"
    ],
    "guard-for-in": 0,
    "max-len": [
      2,
      200,
      2
    ],
    "new-cap": 2,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-cond-assign": [
      2,
      "except-parens"
    ],
    "space-after-keywords": 2,
    "key-spacing": 0,
    "no-debugger": 2,
    "no-empty": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-parens": [2, "functions"],
    "no-irregular-whitespace": 2,
    "no-trailing-spaces": 2,
    "no-iterator": 2,
    "no-loop-func": 2,
    "no-mixed-requires": 0,
    "no-multi-str": 2,
    "no-multi-spaces": [2, { exceptions: { "VariableDeclarator": true } }],
    "no-new": 2,
    "no-plusplus": 0,
    "no-proto": 2,
    "no-script-url": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-use-before-define": [
      2,
      "nofunc"
    ],
    "no-with": 2,
    "quotes": [
      2,
      "single"
    ],
    "semi": [
      2,
      "always"
    ],
    "strict": 0,
    "valid-typeof": 2,
    "wrap-iife": [
      2,
      "inside"
    ],
    "random/arrow-function-parens": 2
  }
}
